{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Job Update Schema",
    "required": [],
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0
        },
        "company_handle": {
            "type": "string"
        }
    },
    "examples": [{
        "title": "Foo",
        "salary": 200000,
        "equity": 0.6,
        "company_handle": "Bar"
    }]
}